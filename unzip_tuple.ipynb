    "# list_a里面的元素是tuple\n",
    "# 可以用以下2种方式解包\n",
    "list_a = [(1, 'a'),(2, 'b'),(3, 'c')]\n",
    "list_b = [4., 5., 6.]\n",
    "# 第1种\n",
    "for i, k in zip(list_a, list_b):\n",
    "    print(i, k)"
